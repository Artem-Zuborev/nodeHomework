{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "sourceMap": true,
    "outDir": "./build",
    "strict": true,
    "noImplicitAny": false,
    "moduleResolution": "node",
    "baseUrl": "tasks2/app.ts",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "lib": ["es2015", "es6"],
  "include": ["task1/**/*"],
  "exclude": ["node_modules"]
}
